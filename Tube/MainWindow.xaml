<Window x:Class="Tube.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tube"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="testButton" Content="Test" HorizontalAlignment="Left" Height="99" Margin="80,80,0,0" VerticalAlignment="Top" Width="162" Click="testButton_Click"/>
        <Button x:Name="loadStations" Content="Load Stations" HorizontalAlignment="Center" Height="99" Margin="0,80,0,0" VerticalAlignment="Top" Width="162" Click="loadStations_Click"/>
        <Button x:Name="findStation" Content="Find Station" HorizontalAlignment="Left" Height="99" Margin="571,80,0,0" VerticalAlignment="Top" Width="162" Click="findStation_Click"/>
        <TextBox x:Name="txtOutput" HorizontalAlignment="Left" Height="189" Margin="80,217,0,0" TextWrapping="Wrap" Text="Output" VerticalAlignment="Top" Width="401"/>
        <TextBox x:Name="txtToFind" HorizontalAlignment="Left" Height="189" Margin="571,217,0,0" TextWrapping="Wrap" Text="Enter station to find" VerticalAlignment="Top" Width="162"/>

    </Grid>
</Window>
